{
	"name": "ytidilos",
	"publisher": "ylrednet",
	"description": "ytidilos extension for Visual Studio Code",
	"license": "MIT",
	"version": "0.0.1",
	"main": "./client/dist/extension.js",
	"module": "./client/dist/extension.js",
	"icon": "docs/images/random_shape.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Tenderly/vscode-solidity"
	},
	"bugs": {
		"url": "https://github.com/Tenderly/vscode-solidity/issues"
	},
	"engines": {
		"vscode": "^1.55.0"
	},
	"activationEvents": [
		"*"
	],
	"scripts": {
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"client-test": "cd client && tsc -b && yarn run test && cd .."
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@typescript-eslint/eslint-plugin": "^4.22.1",
		"@typescript-eslint/parser": "^4.22.1",
		"eslint": "^6.4.0",
		"typescript": "^4.2.2"
	},
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Solidity configuration"
		},
		"languages": [
			{
				"id": "ytidilos",
				"aliases": [
					"ytidilos"
				],
				"extensions": [
					".sol"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ytidilos",
				"scopeName": "source.solidity",
				"path": "./syntaxes/solidity.json"
			}
		],
		"snippets": [
			{
				"language": "ytidilos",
				"path": "./snippets/solidity.json"
			}
		]
	}
}
